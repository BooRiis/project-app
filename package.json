{
  "name": "my-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:app": "nx serve --project=project-app",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && husky install",
    "build": "nx build",
    "build:affected": "nx affected:build --parallel --maxParallel=5",
    "e2e": "nx e2e --all",
    "e2e:cli": "nx e2e --all",
    "e2e:affected:cli": "nx affected:e2e --parallel --maxParallel=5 --all",
    "test": "nx test -all",
    "test:affected": "nx affected:test --parallel --maxParalle=5 -all",
    "pre-commit": "lint-staged",
    "lint": "nx run-many --all --target=lint",
    "lint:fix": "nx affected:lint --fix",
    "lint:affected": "nx affected:lint --parallel --maxParallel=5 --all",
    "prettier": "prettier --write --config ./.prettierrc --ignore-path ./.prettierignore ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.0.0",
    "@angular/common": "~15.0.0",
    "@angular/compiler": "~15.0.0",
    "@angular/core": "~15.0.0",
    "@angular/forms": "~15.0.0",
    "@angular/platform-browser": "~15.0.0",
    "@angular/platform-browser-dynamic": "~15.0.0",
    "@angular/router": "~15.0.0",
    "@babel/core": "^7.20.12",
    "@ngrx/effects": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "@nrwl/angular": "15.4.5",
    "babel-jest": "^28.1.3",
    "husky": "^8.0.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "~15.0.0",
    "@angular/language-service": "~15.0.0",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@nrwl/cypress": "15.4.5",
    "@nrwl/eslint-plugin-nx": "15.4.5",
    "@nrwl/jest": "15.4.5",
    "@nrwl/linter": "15.4.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.4.5",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.3",
    "lint-staged": "^13.1.0",
    "nx": "15.4.5",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.2",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.4",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
